.Receiving_address {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 20rpx 0rpx;
    height: 180rpx;
    background: white;
    padding: 20rpx;
}
.shop{
  background: #fff;  
  padding:20rpx;  
}
.orderListTitle {
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
}
.orderListTitle .userName {
  font-weight: bolder;
  font-size:30rpx;
}
.goods_msg {
    display: flex;
    margin: 20rpx;
    background: red;
    border-radius: 10px;
    padding: 30rpx;
    align-items: center;
}
.orderListDetails {
  display: flex;
  background: #f1f1f1
}
.cartnull{
  text-align: center;
  align-items: center;
  justify-content: center;
  display: flex;
}
.scimg{
  height: 220rpx;
  width: 220rpx;
  margin: 10rpx 10rpx;
}
.goods_img image {
    width: 100%;
}
.goods_content {
    text-align: left;
    height: 200rpx
}
.shop_name{
  display: block;  
  max-height: 80rpx;  
  word-break:break-all;  
  display:-webkit-box;  
  -webkit-line-clamp:2;  
  -webkit-box-orient:vertical;  
  overflow:hidden;
  margin-top: 10rpx;
}
.shop_type{
  display:-webkit-box;  
  font-size: 26rpx;  
  color: gray;
  line-height: 50rpx;
}

.preferential{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 20rpx 20rpx;
    height: 30rpx;
    line-height: 200rpx;
    background: white;
    border-radius: 10rpx;
    padding: 30rpx 0;
    color: black

}

.msg_list {
    background: white;
}
.msg_item {
    display: flex;
    justify-content: space-between;
    margin: 0 20rpx;
    padding: 30rpx 0;
    border-bottom: 2rpx solid #f3f3f3;
}
.msg_item1{
    margin: 0 20rpx;
    padding: 30rpx 0;
    border-bottom: 2rpx solid #f3f3f3;
}
.message{
    display: flex;
    margin: 0 20rpx;
    padding: 30rpx 0;
    border-bottom: 2rpx solid #f3f3f3;
    align-items: center
}
.Total {
    text-align: right;
    margin: 0 20rpx;
    padding: 30rpx 0;
    font-size: 32rpx;
    
}
.Total_price {
    color: #ad0e12;
    font-size: 32rpx;
}
.order_pay {
    display: flex;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 93rpx;
    line-height: 93rpx;
    justify-content: space-between;
    background: white;
 
}
.submit_order {
    width: 32%;
    background: #ad0e12;
    color: white;
    font-size: 32rpx;
    text-align: center;
}
.pay_money {
    font-size: 32rpx;
    margin-left: 30rpx;
}
.bottom{
  padding-top:10rpx;
  padding-bottom: 10rpx;
  border-bottom: 1px solid #f1f1f1
}



.orderListTitle .userName {
  padding-right: 10rpx;
  margin-left:20rpx;
  font-weight: bolder;
    font-size:30rpx;
}

 
.left {
  background: pink;
}
 
.text-style {
  width: 150rpx;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  font-size: 34rpx;
}
 
.active1 {
  color: orange;
  width: 150rpx;
  border-right: 2px solid orange
}
 
