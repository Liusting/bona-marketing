  border-radius: 10px;
  background-color: #fff;
}
.Info{
  background-color: #fff;
  border-radius: 10px;
  width: 100%;
  display: flex
}
.Info .cuponInfo {
  flex: 4;
  margin: 50rpx 10px 20px 30rpx;
  font-size:8px;

}





