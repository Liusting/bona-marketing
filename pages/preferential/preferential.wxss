/*优惠券列表*/
.cuponListDetails {
  display: flex;
  height: 200rpx;
  border-radius: 10px;
  margin: 5px 10px 0px 10px
}
.cuponListDetails .cupon_view {
  display: flex;
  height:200rpx;
  width:130px;
  margin-left: 10rpx;
  text-align: center;
  justify-content: center;
